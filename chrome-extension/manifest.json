{
  "manifest_version": 2,

  "name": "Kupihlepa Crypto Communications",
  "description": "Encrypted communications",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "panel.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://vk.com/*"
      ],
      "css": [
        "css/style.css"
      ],
      "js": [
        "libs/jquery-3.2.1.min.js",
        "libs/socket.io.js",
        "main.js",
        "osint.js"
      ]

    }
  ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js",
    "https://www.vk.com/*"
  ]
}